{
  "version": 2,
  "functions": {
    "api/upload.js": {
      "runtime": "nodejs18.x"
    },
    "api/gen.js": {
      "runtime": "nodejs18.x"
    },
    "api/post.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/upload",
      "methods": ["POST", "OPTIONS"],
      "dest": "api/upload.js"
    },
    {
      "src": "/api/gen",
      "methods": ["POST", "OPTIONS"],
      "dest": "api/gen.js"
    },
    {
      "src": "/api/post",
      "methods": ["POST", "OPTIONS"],
      "dest": "api/post.js"
    }
  ]
}
