{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/upload",
      "methods": ["POST", "OPTIONS"],
      "dest": "api/upload.js"
    },
    {
      "src": "/api/gen",
      "methods": ["POST", "OPTIONS"],
      "dest": "api/gen.js"
    },
    {
      "src": "/api/post",
      "methods": ["POST", "OPTIONS"],
      "dest": "api/post.js"
    }
  ]
}